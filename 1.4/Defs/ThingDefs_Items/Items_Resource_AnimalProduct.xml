<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!--====================  Woolly Stegosaurus ===========================-->
	<ThingDef ParentName="EggFertBase">
		<defName>RG_EggWoollyStegosaurus</defName>
		<label>woolly stegosaurus(fert.)</label>
		<description>A fertilized woolly stegosaurus egg. If all goes well, it should hatch into a baby woolly stegosaurus. It can be eaten raw, but it's much better cooked.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/DinosaurEgg</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<color>(146,102,66)</color>
			<drawSize>0.9</drawSize>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<stackLimit>25</stackLimit>
		<statBases>
			<MarketValue>30</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Hatcher">
				<hatcherDaystoHatch>4.32</hatcherDaystoHatch>
				<hatcherPawn>RG_WoollyStegosaurus</hatcherPawn>
			</li>
		</comps>
	</ThingDef>
