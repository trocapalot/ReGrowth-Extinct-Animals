    <ThingDef ParentName="EggFertBase">
        <defName>RG_ArcticLuizicusEggUnfertilized</defName>
        <label>arctic luizicus (unfert.)</label>
        <description>A unfertilized arctic luizicus egg. It can be eaten raw, but it's much better cooked.</description>
        <graphicData>
            <texPath>Things/Item/Resource/AnimalProductRaw/DinosaurEgg</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
            <color>(146,102,66)</color>
            <drawSize>(1.2,1.2)</drawSize>
        </graphicData>
        <socialPropernessMatters>true</socialPropernessMatters>
        <statBases>
            <MarketValue>95.0</MarketValue>
