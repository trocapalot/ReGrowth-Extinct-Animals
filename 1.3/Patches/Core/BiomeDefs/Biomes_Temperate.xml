<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Core</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/BiomeDef[defName="TemperateForest"]/wildAnimals</xpath>
                    <value>
                        <RG_Dodo>0.05</RG_Dodo>
						<RG-Protoceratidae>0.2</RG-Protoceratidae>
                        <RG_TerrorBird>0.3</RG_TerrorBird>
						<RG_Megalenhydris>0.5</RG_Megalenhydris>
						<RG_Thrinaxodon>0.8</RG_Thrinaxodon>
						<RG_Velociraptor>0.2</RG_Velociraptor>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/BiomeDef[defName="TemperateSwamp"]/wildAnimals</xpath>
                    <value>
                        <RG_TerrorBird>0.1</RG_TerrorBird>
						<RG_Megalenhydris>0.2</RG_Megalenhydris>
						<RG_Thrinaxodon>0.4</RG_Thrinaxodon>
						<RG_Velociraptor>0.1</RG_Velociraptor>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>